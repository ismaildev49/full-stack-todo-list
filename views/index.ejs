    <!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Your todo list</h1>
    
        <ul>
            <% todos.forEach((todo) => { %>
            <li data-id="<%= todo._id %>"><p><%= todo.content %></p><button class="delete-btn">Delete</button></li>
            <% }) %>
        </ul>
   

    <script src="/app.js"></script>
</body>
</html>
</body>
